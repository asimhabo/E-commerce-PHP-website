								<li><a href="/shopcart"><i class="fa fa-shopping-cart"></i> My Shopcart</a></li>
								{% if app.user %}
								<li class="dropdown"><a href="#">{{ app.user.name }}<i class="fa fa-angle-down"></i></a>
								<li><a href="/logout"><i class="fa fa-lock"></i> Logout</a></li>
								{% else %}
								<li><a href="/login"><i class="fa fa-lock"></i> Login</a></li>
								{% endif %}
							</ul>
						</div>
					</div>
				</div>
			</div>
